{
    "name": "balise/anchor",
    "description": "A Wordpress Boilerplate",
    "type": "project",
    "require": {
        "balise/anchor-framework": ">0"
    },
    "scripts": {
        "post-root-package-install": [
            "npm install",
            "mkdir -p app/content/fields",
            "mkdir -p app/content/navigations",
            "mkdir -p app/content/posttypes",
            "mkdir -p app/content/taxonomies",
            "mkdir -p app/includes",
            "mkdir -p app/js",
            "mkdir -p app/css",
            "mkdir -p app/less",
            "mkdir -p app/scss",
            "mkdir -p app/views",
            "mkdir -p components",
            "mkdir -p public/css",
            "mkdir -p public/editor",
            "mkdir -p public/js",
            "mkdir -p public/views"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "prefer-lowest": false,
    "license": "MIT",
    "homepage": "https://balise.ca"
}
